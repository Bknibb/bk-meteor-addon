{
  "schemaVersion": 1,
  "id": "bk-meteor-addon",
  "version": "${version}",
  "name": "Bk Meteor Addon",
  "authors": [
    "bknibb"
  ],
  "contact": {
    "repo": "https://github.com/Bknibb/bk-meteor-addon"
  },
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "org.bknibb.bk_meteor_addon.BkMeteorAddon"
    ]
  },
  "mixins": [
    "bk-meteor-addon.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "25,255,255"
  },
  "depends": {
    "java": ">=21",
    "minecraft": ["${mc_version}"],
    "meteor-client": "*"
  }
}
